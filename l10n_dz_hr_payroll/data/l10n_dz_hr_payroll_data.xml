<!-- HR SALARY RULES-->
    <!-- Salaire de base-->
    <record id="hr_payroll.hr_rule_basic" model="hr.salary.rule">
        <field name="name">Salaire de base</field>
        <field name="code">R001</field>
        <field name="sequence">1</field>
        <field name="amount_select">percentage</field>
        <field name="quantity">worked_days.WORK100.number_of_days</field>
        <field name="amount_percentage_base">(contract.wage / worked_days.WORK100.number_of_days)</field>
        <field name="amount_percentage" eval="100"/>
        <field name="register_id" ref="hr_payroll.contrib_register_employees"/>
    </record>

    <record id="hr_rule_base_g" model="hr.salary.rule">
        <field name="name">Regul. Salaire de base (+)</field>
        <field name="category_id" ref="BASE_G"/>
        <field name="code">R002</field>
        <field name="sequence">2</field>
        <field name="condition_select">python</field>
        <field name="condition_python">result = (inputs.I002 and inputs.I002.amount) != 0.0</field>
    	<field name="amount_select">code</field>
        <field name="amount_python_compute">result = inputs.I002.amount</field>
        <field name="register_id" ref="hr_payroll.contrib_register_employees"/>
    </record>
        <record id="hr_rule_input_rsbg" model="hr.rule.input">
            <field name="code">I002</field>
            <field name="name">Regul. salaire de base (+)</field>
            <field name="input_id" ref="hr_rule_base_g"/>
        </record>

    <record id="hr_rule_base_r" model="hr.salary.rule">
        <field name="name">Regul. Salaire de base (-)</field>
        <field name="category_id" ref="BASE_R"/>
        <field name="code">R003</field>
        <field name="sequence">3</field>
        <field name="condition_select">python</field>
        <field name="condition_python">result = (inputs.I003 and inputs.I003.amount) != 0.0 </field>
    	<field name="amount_select">code</field>
        <field name="amount_python_compute">result = -(inputs.I003.amount)</field>
        <field name="register_id" ref="hr_payroll.contrib_register_employees"/>
    </record>
       <record id="hr_rule_input_rsbr" model="hr.rule.input">
